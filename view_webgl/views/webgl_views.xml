<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="webgl_view_users">
        <field name="name">Webgl View</field>
        <field name="model">res.users</field>
        <field name="arch" type="xml">
            <webgl name="Webgl view"/>
        </field>
    </record>
    <record model="ir.ui.view" id="webgl_view_partners">
        <field name="name">Webgl View</field>
        <field name="model">res.users</field>
        <field name="arch" type="xml">
            <webgl name="Webgl view"/>
        </field>
    </record>
    <record model="ir.actions.act_window" id="base.action_res_users">
        <field name="name">Webgl Users</field>
        <field name="res_model">res.users</field>
        <field name="view_mode">tree,form,webgl</field>
    </record>

    <record model="ir.actions.act_window" id="base.action_partner_form">
        <field name="name">Webgl Customers</field>
        <field name="res_model">res.partner</field>
        <field name="view_mode">kanban,tree,form,webgl</field>
    </record>
</odoo>
